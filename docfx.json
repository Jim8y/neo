{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/**.csproj"],
          "exclude": ["**/bin/**", "**/obj/**"]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "toc.yml", "*.md"],
        "exclude": ["**/bin/**", "**/obj/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "dest": "_site/markdown",
    "template": ["markdown"]
  }
}
